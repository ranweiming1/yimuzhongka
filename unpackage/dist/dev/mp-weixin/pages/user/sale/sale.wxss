.yes {
  width: 375rpx;
  text-align: center;
  float: left;
  padding-bottom: 20rpx;
}
.yes text {
  width: 32rpx;
  color: #333;
  line-height: 80rpx;
}
.yes .imgBox image {
  width: 34rpx;
  height: 6rpx;
  display: block;
  margin: 0 auto;
}
.none {
  width: 375rpx;
  text-align: center;
  float: left;
}
.none text {
  width: 32rpx;
  color: #999;
  line-height: 80rpx;
}
.valid {
  overflow: hidden;
  position: relative;
  width: 710rpx;
  height: 205rpx;
  margin: 20rpx;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  background-color: #fff;
  -webkit-box-shadow: 0 0 5px #ccc;
          box-shadow: 0 0 5px #ccc;
}
.valid .imgBox {
  float: left;
  padding-right: 20rpx;
}
.valid .imgBox image {
  width: 200rpx;
  height: 205rpx;
  display: block;
}
.valid .imgBox .sum {
  position: absolute;
  top: 15%;
  left: 6%;
  text-align: center;
}
.valid .imgBox .sum text {
  font-size: 60rpx;
  color: #fff;
}
.valid .imgBox .sum text text {
  font-size: 30rpx;
  color: #fff;
}
.valid .imgBox .sum .tianjan text {
  color: #fff;
  font-size: 20rpx;
  line-height: 50rpx;
}
.valid .float {
  float: left;
  width: 50%;
}
.valid .float .h2 {
  font-size: 32rpx;
  color: #333;
  line-height: 50rpx;
  padding-top: 30rpx;
}
.valid .float .p {
  font-size: 24rpx;
  color: #666;
  line-height: 50rpx;
}
.valid .float .span {
  font-size: 20rpx;
  color: #999;
  line-height: 30rpx;
}
.valid .bott {
  float: right;
  color: #fff;
  margin: 80rpx 20rpx;
  padding: 10rpx 20rpx;
  -webkit-border-radius: 30rpx;
          border-radius: 30rpx;
  font-size: 22rpx;
  background: -webkit-gradient(linear, left top, right top, from(#ff9429), to(#ff6759));
  background: -webkit-linear-gradient(left, #ff9429, #ff6759);
  background: linear-gradient(to right, #ff9429, #ff6759);
}
