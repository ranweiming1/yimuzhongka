}
.guanb text {
  font-size: 28rpx;
  color: #ff0000;
}
.bottBox {
  overflow: hidden;
  width: 710rpx;
  padding: 20rpx;
  height: 50rpx;
}
.bottBox .onna button {
  background-color: #007AFF !important;
  border: 1px solid #007AFF !important;
  color: #fff !important;
}
.bottBox .onnb button {
  background-color: #fff !important;
