.top {
  width: 710rpx;
  margin: 20rpx;
  overflow: hidden;
}
.top .textBox {
  float: left;
  margin-left: 70rpx;
  background-color: #f0f0f0;
  border-radius: 50rpx;
}
.top .textBox input {
  height: 60rpx;
  width: 520rpx;
  line-height: 60rpx;
  padding-left: 20rpx;
  font-size: 26rpx;
}
.top .textBox .uni-input-placeholder {
  color: #999 !important;
}
.top .imgBox {
  padding-left: 30rpx;
  padding-right: 20rpx;
  padding-top: 10rpx;
  float: right;
}
.top .imgBox image {
  width: 44rpx;
  height: 39rpx;
}
.topBox {
  overflow: hidden;
  border-bottom: 1px solid #f0f0f0;
}
.topBox .none {
  text-align: center;
  float: left;
  width: 150rpx;
}
.topBox .none .ontext text {
  font-size: 30rpx;
  color: #333;
}
.topBox .none .onimg {
  padding-left: 60rpx;
  padding-top: 15rpx;
}
.topBox .none .onimg image {
  display: block;
  width: 34rpx;
  height: 6rpx;
}
.xinxi {
  overflow: hidden;
  width: 710rpx;
  padding: 20rpx;
  border-bottom: 20rpx solid #f7f7f7;
}
.xinxi .imgBox_a {
  float: left;
  padding-top: 20rpx;
}
.xinxi .imgBox_a image {
  width: 215rpx;
  height: 160rpx;
}
.xinxi .zongj {
  float: right;
  padding-top: 10rpx;
}
.xinxi .zongj text {
  font-size: 26rpx;
  color: #333;
}
.xinxi .txt_c {
  float: left;
  width: 470rpx;
  padding-left: 20rpx;
}
.xinxi .txt_c .title {
  padding-top: 10rpx;
}
.xinxi .txt_c .title text {
  font-size: 30rpx;
  line-height: 30rpx;
  color: #333;
}
.xinxi .txt_c .spec {
  font-size: 26rpx;
  line-height: 40rpx;
  color: #666;
}
.xinxi .txt_c .radColor {
  float: left;
  color: #ff0000;
  font-size: 32rpx;
}
.xinxi .txt_c .jia {
  float: right;
}
.xinxi .txt_c .jia text {
  font-size: 20rpx;
  color: #666;
}
.radios {
  width: 710rpx;
  padding: 20rpx;
