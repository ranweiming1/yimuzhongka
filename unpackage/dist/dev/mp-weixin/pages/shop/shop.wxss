.bg {
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}
.bg image {
  width: 750rpx;
  height: 450rpx;
}
.top {
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  z-index: 99;
  overflow: hidden;
}
.top .textBox {
  float: left;
  background-color: rgba(255, 255, 255, 0.6);
  -webkit-border-radius: 50rpx;
          border-radius: 50rpx;
}
.top .textBox input {
  height: 60rpx;
  width: 520rpx;
  line-height: 60rpx;
  padding-left: 20rpx;
  font-size: 26rpx;
}
.top .textBox .uni-input-placeholder {
  color: #ffffff !important;
}
.top .imgBox {
  padding-left: 30rpx;
  padding-right: 20rpx;
  float: right;
}
.top .imgBox image {
  width: 40rpx;
  height: 7rpx;
}
.head {
  width: 710rpx;
  position: absolute;
  left: 30rpx;
  top: 150rpx;
}
.head .head_img {
  float: left;
}
.head .head_img image {
  width: 115rpx;
  height: 115rpx;
  -webkit-border-radius: 5rpx;
          border-radius: 5rpx;
}
.head .head_text {
  float: left;
  padding-left: 20rpx;
}
.head .head_text .h2 text {
  font-size: 36rpx;
  color: #fff;
}
.head .head_text .star1 {
  padding-top: 20rpx;
}
.head .head_text .star1 image {
  width: 216rpx;
  height: 36rpx !important;
  display: block;
}
.head .collect {
  float: right;
  margin-top: 30rpx;
  text-align: center;
  width: 150rpx;
  height: 50rpx;
  border: 1px solid #fff;
  -webkit-border-radius: 40rpx;
          border-radius: 40rpx;
}
.head .collect text {
  padding-bottom: 20rpx;
  overflow: hidden;
  font-size: 20rpx;
  color: #fff;
  line-height: 30rpx;
}
.banner {
  width: 93%;
  position: absolute;
  top: 300rpx;
  left: 30rpx;
  margin: 0 auto;
  display: block;
  -webkit-box-shadow: 0 0 5px #999;
          box-shadow: 0 0 5px #999;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
}
.banner image {
  width: 100%;
  height: 262rpx;
  margin: 0 auto;
  display: block;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
}
.ticket_a {
  position: absolute;
  top: 580rpx;
  left: 30rpx;
}
.ticket_a image {
  width: 690rpx;
  height: 205rpx;
}
.BoxBox {
  background-color: #fff;
  position: absolute;
  top: 800rpx;
  left: 0rpx;
  margin-bottom: 70rpx;
}
.biaot {
  border-bottom: 1px solid #ccc;
  font-size: 30rpx;
  line-height: 80rpx;
  padding-left: 20rpx;
}
.biaot .dele {
  float: right;
  padding-right: 20rpx;
}
.biaot .dele text {
  font-size: 26rpx;
  color: #666;
  line-height: 50rpx;
}
.list {
  width: 710rpx;
  margin: 20rpx;
  overflow: hidden;
  padding-top: 10rpx;
}
.list .content {
  display: black;
  width: 100%;
  border-bottom: 1px dotted #ccc;
  padding-bottom: 20rpx;
  overflow: hidden;
}
.list .content .imgBox image {
  width: 30%;
  height: inherit;
  float: left;
}
.list .content .txt_a {
  padding-top: 10rpx;
  width: 67%;
  margin-left: 20rpx;
  float: left;
}
.list .content .txt_a text {
  font-size: 30rpx;
}
.list .content .txt_a .span_a {
  background-color: #fff;
  color: #ff6600;
  border: 1px solid #ff6600;
  -webkit-border-radius: 5rpx;
          border-radius: 5rpx;
  padding: 5rpx 10rpx;
  font-size: 18rpx;
  margin-right: 10rpx;
  line-height: 30rpx;
}
.list .content .txt_a .txt_aa text {
  border: 1px dotted #ff3333;
  background-color: #fff;
  font-size: 16rpx;
  line-height: 30rpx;
  padding: 5rpx 10rpx;
  color: #ff3333;
  margin-right: 20rpx;
  font-size: 20rpx;
}
.list .content .txt_a .txt_aas {
  padding-top: 10rpx;
}
.list .content .txt_a .txt_aas text {
  color: #333;
  font-size: 32rpx;
}
.list .content .txt_a .txt_aas text text {
  color: #ff3333;
  font-size: 32rpx;
  font-weight: bold;
}
.list .content .txt_a .txt_aas text:nth-child(2) {
  color: #999999;
  font-size: 24rpx;
  float: right;
  line-height: 50rpx;
}

