.top {
  width: 750rpx;
  margin: 0 auto;
  overflow: hidden;
  border-bottom: 1px solid #e5e5e5;
}
.top .textBox {
  padding-left: 40%;
}
.top .textBox text {
  font-size: 38rpx;
  color: #333;
  float: left;
  line-height: 90rpx;
}
.top .imgBox {
  float: right;
  padding: 30rpx;
}
.top .imgBox image {
  width: 36rpx;
  height: 36rpx;
}
