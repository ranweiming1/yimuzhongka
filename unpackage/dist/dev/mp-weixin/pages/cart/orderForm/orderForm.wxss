.noneBox {
  width: 250rpx;
  text-align: center;
  margin: 0 auto;
  padding-top: 20rpx;
  padding-bottom: 40rpx;
  padding-bottom: 20rpx;
}
.noneBox text {
  display: block;
  border: 1px solid #007AFF;
  color: #007AFF;
  background-color: #fff;
  padding: 15rpx 20rpx;
  font-size: 26rpx;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  margin-bottom: 20rpx;
}
.siteBox {
  width: 710rpx;
  height: initial;
  padding: 20rpx;
  background-color: #fff;
  overflow: hidden;
  border-bottom: 1px solid #ccc;
}
.siteBox .content {
  float: left;
  width: 80%;
}
.siteBox .content .nome {
  float: left;
  font-size: 30rpx;
  color: #333;
  padding-right: 10rpx;
}
.siteBox .content .call {
  float: left;
  font-size: 24rpx;
  color: #666;
  line-height: 40rpx;
  padding-right: 10rpx;
}
.siteBox .content .label {
  float: left;
  font-size: 18rpx;
  color: #ff6633;
  padding: 5rpx;
  margin-top: 5rpx;
  background-color: #fdf1ec;
}
.siteBox .content .p {
  float: left;
  font-size: 26rpx;
  color: #333;
  line-height: 40rpx;
}
.xinxi {
  overflow: hidden;
  width: 710rpx;
  padding: 20rpx;
  border-bottom: 1px dotted #ccc;
}
.xinxi .biaoyi text {
  font-size: 28rpx;
}
.xinxi .imgBox_a {
  float: left;
  padding-top: 20rpx;
}
.xinxi .imgBox_a image {
  width: 215rpx;
  height: 160rpx;
}
.xinxi .txt_c {
  float: left;
  width: 460rpx;
  padding-left: 20rpx;
}
.xinxi .txt_c .title {
  padding-top: 10rpx;
}
.xinxi .txt_c .title text {
  font-size: 30rpx;
  line-height: 30rpx;
  color: #333;
}
.xinxi .txt_c .spec {
  font-size: 26rpx;
  line-height: 40rpx;
  color: #666;
}
.xinxi .txt_c .radColor {
  float: left;
  color: #ff0000;
  font-size: 32rpx;
}
.xinxi .txt_c .jia {
  float: right;
}
.xinxi .txt_c .jia text {
  font-size: 20rpx;
  color: #666;
}
.basic {
  width: 710rpx;
  background-color: #fff;
  overflow: hidden;
  padding: 20rpx;
  padding-bottom: 10rpx;
  border-bottom: 20rpx solid #f7f7f7;
}
.basic .left_a {
  float: left;
}
.basic .left_a text {
  font-size: 28rpx;
  color: #333;
  line-height: 50rpx;
}
.basic .right_a {
  float: right;
  padding-right: 10rpx;
}
.basic .right_a text {
  font-size: 28rpx;
  float: right;
  line-height: 50rpx;
}
.basic .right_a text text {
  color: #ff670c;
}
.basic .right_a .img_l {
  float: left;
  padding-right: 10rpx;
  padding-top: 5rpx;
}
.basic .right_a .img_l image {
  width: 45rpx;
  height: 45rpx;
}
.basic .right_a .img_a {
  float: right;
  padding: 20rpx 0rpx 20rpx 20rpx;
}
.basic .right_a .img_a image {
  width: 12rpx;
  height: 14rpx;
  display: block;
}
.uni-column {
  background-color: #fff;
  overflow: hidden;
  border-bottom: 20rpx solid #f7f7f7;
}
.uni-column .title {
  float: left;
  padding-left: 20rpx;
}
.uni-column .title text {
  line-height: 90rpx;
  font-size: 28rpx;
}
.uni-column .uni-input {
  float: left;
  padding-top: 27rpx;
  font-size: 28rpx;
  padding-left: 20rpx;
  font-family: Microsoft YaHei;
}
.uni-column .imgBox {
  float: left;
  padding-top: 30rpx;
  padding-left: 20rpx;
}
.uni-column .imgBox image {
  width: 175rpx;
  height: 175rpx;
}
.aa {
  border-bottom: 1px dotted #ccc;
}
.ssa {
  margin-bottom: 100rpx;
}
.bottom {
  width: 750rpx;
  height: 100rpx;
  background-color: #fff;
  border-top: 1px solid #ccc;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
}
.bottom .leftA {
  float: left;
  padding-left: 20rpx;
}
.bottom .leftA .ppp {
  float: left;
}
.bottom .leftA .ppp text {
  font-size: 28rpx;
  color: #333;
  line-height: 90rpx;
}
.bottom .leftA .ppp text text {
  font-size: 28rpx;
  color: #ff0000;
}
.bottom .leftA .yunf {
  float: left;
  padding-left: 10rpx;
}
.bottom .leftA .yunf text {
  line-height: 90rpx;
  font-size: 24rpx;
  color: #999;
}
.bottom .rightA {
  float: right;
  width: 260rpx;
  height: 100%;
  text-align: center;
  background-color: #2b5cff;
}
.bottom .rightA text {
  font-size: 30rpx;
  color: #fff;
  line-height: 90rpx;
}

